{
  "name": "kooki",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "db:migrate": "pnpm --filter @kooki/api exec prisma migrate dev",
    "db:push": "pnpm --filter @kooki/api exec prisma db push",
    "db:seed": "pnpm --filter @kooki/api exec prisma db seed",
    "db:studio": "pnpm --filter @kooki/api exec prisma studio"
  },
  "devDependencies": {
    "turbo": "^2.4.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  }
}
